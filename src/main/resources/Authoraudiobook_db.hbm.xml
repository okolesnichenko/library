<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.Authoraudiobook_db" table="authoraudiobook" schema="library_db">
        <composite-id mapped="true" class="Entity.Authoraudiobook_dbPK">
            <key-property name="idAudiobook" column="ID_audiobook"/>
            <key-property name="idAuthor" column="ID_author"/>
        </composite-id>
    </class>
</hibernate-mapping>